{
	"name": "IOT_Run Databricks to sql",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"state": "Inactive",
				"onInactiveMarkAs": "Succeeded",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureDatabricksDeltaLakeSource",
						"query": "select * from iottest.gold.gold_jobcosting_run",
						"exportSettings": {
							"type": "AzureDatabricksDeltaLakeExportCommand"
						}
					},
					"sink": {
						"type": "SqlServerSink",
						"writeBehavior": "upsert",
						"upsertSettings": {
							"useTempDB": true
						},
						"sqlWriterUseTableLock": false
					},
					"enableStaging": true,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "start",
									"type": "DateTime"
								},
								"sink": {
									"name": "start",
									"type": "DateTimeOffset"
								}
							},
							{
								"source": {
									"name": "end",
									"type": "DateTime"
								},
								"sink": {
									"name": "end",
									"type": "DateTimeOffset"
								}
							},
							{
								"source": {
									"name": "note",
									"type": "String"
								},
								"sink": {
									"name": "note",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "machineId",
									"type": "Int"
								},
								"sink": {
									"name": "machineId",
									"type": "Int32"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "AzureDatabricksDeltaLake",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "wwsql003_Run_IOT",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}